<?xml version="1.0"?>
<!--
  The complete turtlebot library of xacros for easy reference
 -->
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <!-- General -->
  <xacro:include filename="$(find IRL_learning_ros)/urdf/common_properties.urdf.xacro"/>
  <xacro:include filename="$(find IRL_learning_ros)/urdf/turtlebot_properties.urdf.xacro"/>
  <!-- Bases -->
  <xacro:include filename="$(find IRL_learning_ros)/urdf/kobuki.urdf.xacro" />
  <!-- Stacks -->
  <xacro:include filename="$(find IRL_learning_ros)/urdf/hexagons.urdf.xacro"/>
  <!-- 3D Sensors -->
  <xacro:include filename="$(find IRL_learning_ros)/urdf/sensor/asus_xtion_pro.urdf.xacro"/>
  <!-- 2D Rplidar -->
  <xacro:include filename="$(find IRL_learning_ros)/urdf/sensor/rplidar.urdf.xacro"/>
</robot>
